{
  "presets": [
    "@babel/env",
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["module-resolver", {
        "alias": {
          "img": "./src/img",
          "client": "./src/openapi/client"
        }
      }]
  ]
}
